	FROM dwa_wind_site_1d
	WHERE date >= concat(date_format('${para_date}','%Y-%m'),'-01')
	AND date <= '${para_date}'
	GROUP BY data_m_fmt
) seg1 ON date_format(dws1.date,'%Y-%m') = seg1.data_m_fmt
LEFT JOIN (
