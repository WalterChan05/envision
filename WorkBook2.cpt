		,IFNULL(SUM(budget_production),0) m_rm_bgt  -- 黑山月计划
		,y_rm_bgt -- 黑山年计划
	FROM dim_wind_site_month_attr dwsma 
	LEFT JOIN (
		SELECT 
			 SUBSTR(date_str, 1,4) date_ytd
			,IFNULL(ROUND(SUM(budget_production),2),0) y_rm_bgt
		FROM dim_wind_site_month_attr dwsma 
		WHERE end_date IS NULL 
		GROUP BY date_ytd
