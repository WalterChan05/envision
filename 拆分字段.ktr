    <sql>
SELECT
		 ZPS_AUTOID
		,ZPS_ORG_NAME 单位名称
		,ZPS_POS_NAME 职位名称
		,ZPS_RECRUITMENT_NUM 招考人数
		,ZPS_MAJOR 专业
		,ZPS_EDU_BACK 学历
		,ZPS_EDU_DEGREE 学位
		,ZPS_AGE 年龄
		,ZPS_GENDER 性别
		,ZPS_OBJECT 招考对象
		,ZPS_NOTE 备注 
		,ZPS_CATEGORY 考试类别
		,ZPS_YEAR 年份 
		,ZPS_CATE_PUB 公共科目考试类别
		,ZPS_CATE_MAJ 专业科目考试类别
		,REGEXP_COUNT(ZPS.ZPS_MAJOR, '[^、；;,，。]+') ZM_CT

FROM
		ZJ_PUBLIC_SERVANT ZPS
	
 WHERE  ZPS.ZPS_YEAR = '2017'
 AND ZPS.ZPS_MAJOR LIKE '%审计学%'

-- AND ZPS.ZPS_AUTOID = '81517b48-674a-e5c9-e050-1fac6e716d34' 
</sql>
